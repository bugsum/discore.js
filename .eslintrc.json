{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "plugins": ["@typescript-eslint"],
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/no-unused-vars": "warn",
        "no-console": "warn"
    }
}
